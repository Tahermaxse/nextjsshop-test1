{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/**/*",
    "components/sparkles.js",
    "app/api/signup/route.js",
    "app/api/auth/[...nextauth]/route.js",
    "middleware.js",
    "lib/auth.js"
, "components/GitHubAccessDialog.tsx", "scripts/build-registry.ts", "app/api/auth/[...nextauth]/auth.ts", "app/docs/Docs"  ],
  "exclude": [
    "node_modules"
  ]
}